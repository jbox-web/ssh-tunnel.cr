<!-- Created with Cambalache 0.10.3 -->
<interface>
  <requires lib="gtk" version="4.6"/>
  <template class="SSHTunnel-UI-Windows-Tunnels-DeleteWindow" parent="GtkWindow">
    <property name="default-height">100</property>
    <property name="default-width">350</property>
    <property name="modal">True</property>
    <property name="resizable">False</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="label.confirm">
            <property name="label" translatable="yes">Are you sure?</property>
            <property name="vexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkActionBar">
            <child>
              <object class="GtkButton" id="button.cancel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">gtk-cancel</property>
                <property name="valign">baseline</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button.submit">
                <property name="halign">end</property>
                <property name="label" translatable="yes">gtk-apply</property>
                <property name="valign">baseline</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
