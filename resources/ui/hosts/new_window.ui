<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.3 -->
<interface>
  <requires lib="gtk" version="4.6"/>
  <template class="SSHTunnel-UI-Windows-Hosts-NewWindow" parent="GtkWindow">
    <property name="modal">True</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkGrid">
            <child>
              <object class="GtkLabel" id="label.host.name">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Name</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-start">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="input.host.name">
                <property name="hexpand">True</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">0</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="error.host.name">
                <property name="margin-end">10</property>
                <layout>
                  <property name="column">2</property>
                  <property name="column-span">1</property>
                  <property name="row">0</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label.host.user">
                <property name="halign">start</property>
                <property name="label" translatable="yes">User</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-start">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="input.host.user">
                <property name="hexpand">True</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="error.host.user">
                <property name="margin-end">10</property>
                <layout>
                  <property name="column">2</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label.host.host">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Host</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-start">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="input.host.host">
                <property name="hexpand">True</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="error.host.host">
                <property name="margin-end">10</property>
                <layout>
                  <property name="column">2</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label.host.port">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Port</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-start">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">3</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="input.host.port">
                <property name="hexpand">True</property>
                <property name="input-purpose">digits</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">3</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="error.host.port">
                <property name="margin-end">10</property>
                <layout>
                  <property name="column">2</property>
                  <property name="column-span">1</property>
                  <property name="row">3</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label.host.identity_file">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Identity file</property>
                <property name="margin-bottom">5</property>
                <property name="margin-end">10</property>
                <property name="margin-start">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">4</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button.host.select_identity_file">
                <property name="hexpand">True</property>
                <property name="label">Select File</property>
                <property name="margin-end">10</property>
                <property name="margin-top">3</property>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">4</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button.host.reset_identity_file">
                <property name="label" translatable="yes">Reset</property>
                <property name="margin-end">10</property>
                <property name="margin-top">5</property>
                <layout>
                  <property name="column">2</property>
                  <property name="column-span">1</property>
                  <property name="row">4</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="margin-top">5</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkScrolledWindow" id="scrolled_window.tunnels">
                <property name="hexpand">True</property>
                <property name="propagate-natural-height">True</property>
                <property name="propagate-natural-width">True</property>
                <property name="vexpand">True</property>
              </object>
            </child>
            <child>
              <object class="GtkActionBar">
                <child>
                  <object class="GtkButton" id="button.tunnel.new">
                    <property name="icon-name">list-add-symbolic</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="button.tunnel.edit">
                    <property name="icon-name">document-edit-symbolic</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="button.tunnel.delete">
                    <property name="icon-name">app-remove-symbolic</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkActionBar">
            <property name="valign">baseline</property>
            <child>
              <object class="GtkButton" id="button.cancel">
                <property name="focusable">True</property>
                <property name="halign">start</property>
                <property name="label" translatable="yes">gtk-cancel</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button.submit">
                <property name="halign">end</property>
                <property name="label" translatable="yes">gtk-apply</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
